# Auto detect text files and perform LF normalization
* text=auto
# Known text files
*.java text
*.xml text
*.yml text
*.md text
*.properties text
*.sh text
# Known binary files
*.jar binary
*.gif binary
build.xml filter=lfs diff=lfs merge=lfs -text
CleanSpec.mk filter=lfs diff=lfs merge=lfs -text
common.xml filter=lfs diff=lfs merge=lfs -text
lib filter=lfs diff=lfs merge=lfs -text
NOTICE filter=lfs diff=lfs merge=lfs -text
CONTRIBUTING.md filter=lfs diff=lfs merge=lfs -text
examples filter=lfs diff=lfs merge=lfs -text
extensions filter=lfs diff=lfs merge=lfs -text
jdk8-tests filter=lfs diff=lfs merge=lfs -text
METADATA filter=lfs diff=lfs merge=lfs -text
OWNERS filter=lfs diff=lfs merge=lfs -text
README.md filter=lfs diff=lfs merge=lfs -text
util filter=lfs diff=lfs merge=lfs -text
bom filter=lfs diff=lfs merge=lfs -text
build.properties filter=lfs diff=lfs merge=lfs -text
COPYING filter=lfs diff=lfs merge=lfs -text
core filter=lfs diff=lfs merge=lfs -text
latest-api-diffs filter=lfs diff=lfs merge=lfs -text
